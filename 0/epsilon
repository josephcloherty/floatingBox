FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    "(sides|bottom)"
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }

	"(front|back)"
	{
		type			empty;
	}

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0.1;
        value           uniform 0.1;
    }

    floatingObject
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }
}

#include $WM_PROJECT_DIR/etc/caseDicts/setConstraintTypes
