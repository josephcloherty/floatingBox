FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "(sides|bottom)"
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

	"(front|back)"
	{
		type			empty;
	}

    atmosphere
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    floatingObject
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}

#include $WM_PROJECT_DIR/etc/caseDicts/setConstraintTypes
