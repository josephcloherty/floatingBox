FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];


internalField   uniform 0.1;

boundaryField
{
    back
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }
    sides
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }
    front
    {
        type            epsilonWallFunction;
        value           uniform 0.1;
    }
    atmosphere
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    floatingObject
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary7to8
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary7to9
    {
        type            processor;
        value           uniform 0.1;
    }
}


