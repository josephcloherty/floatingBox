FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            noSlip;
    }
    sides
    {
        type            noSlip;
    }
    bottom
    {
        type            noSlip;
    }
    front
    {
        type            noSlip;
    }
    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           nonuniform List<vector> 0();
    }
    floatingObject
    {
        type            movingWallVelocity;
        value           nonuniform List<vector> 0();
    }
    procBoundary10to9
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary10to11
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


