FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "2.7";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    sides
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    front
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    floatingObject
    {
        type            calculated;
    }
    procBoundary4to0
    {
        type            processor;
    }
    procBoundary4to3
    {
        type            processor;
    }
    procBoundary4to5
    {
        type            processor;
    }
}


